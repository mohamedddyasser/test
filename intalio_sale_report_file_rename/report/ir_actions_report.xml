<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sale.action_report_saleorder" model="ir.actions.report">
        <field name="print_report_name">object._get_report_filename() or (object.state in ('draft', 'sent') and 'Quotation - %s' % (object.name)) or 'Order - %s' % (object.name)</field>
    </record>
</odoo>
